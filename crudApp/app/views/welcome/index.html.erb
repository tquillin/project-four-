<div class="navigation">
  <h1>Music Life</h1>
  <!-- <p></p> -->

</div>



<body ng-controller="main">
<div id="alerts" class="row">
      <form class="col s12" id="newsletter" action="index.html" method="post">
        <h5>Sign up for email alerts</h5>
        <input class="col s12" type="text" name="name" value="" placeholder="...email">
        <button type="submit" name="button">register</button>
      </form>
</div>

<div class="row">
      <form id="" class="col s12" ng-submit="create()">
            <div id="new-entry" class="col s6">
              <input id="nickname" class="col s5" type="text" ng-model="newSong.username" value="" placeholder="...nickname">
              <textarea id="entry" type="text" ng-model="newSong.description" value="" placeholder="...what's on your mind?" cols="40"></textarea>
              <button type="submit" name="button">submit</button>

            </div>
      </form>
</div>

<div class="row">
    <div id="results" class="col s12">

      <ul>
        <span>
            <li class="col s3" ng-repeat="song in songs">

                <h5>User: {{song.username}}</h5>
                <p class="paragraph">{{song.description}}</p>
                <!-- <i id="add" class="material-icons">thumb_up</i> -->

              <a href="#" ng-click="destroy(song.id)">Delete</a>
            </li>
      </span>

      </ul>

      <!-- <br /> -->
    </div>
</div>

<!-- <span></span> -->




</body>
